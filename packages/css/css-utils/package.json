{
  "name": "@novicell/css-utils",
  "version": "1.0.2",
  "description": "A CSS utility class package inspired by Bootstrap 4 CSS utilities and configurable via CSS Custom Properties and PostCSS.",
  "author": "Simon NÃ¸rlund Eldevig <sne@novicell.dk>",
  "homepage": "https://novicell-css-utils.netlify.app",
  "main": "dist/all.css",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=compile parcel index.html --no-cache",
    "build": "npm run lint:fix && parcel build src/*.css --no-content-hash --no-source-maps --no-minify -d dist/src --no-cache",
    "build:html": "cross-env NODE_ENV=compile parcel build index.html --no-cache",
    "compile": "npm run lint:fix && cross-env NODE_ENV=compile parcel build src/*.css --no-content-hash --no-source-maps --no-cache",
    "generate": "npm run build && npm run compile",
    "lint": "stylelint 'src/**/*.css'",
    "lint:fix": "stylelint 'src/**/*.css' --fix",
    "prepublishOnly": "npm run generate"
  },
  "keywords": [
    "css",
    "cssutils",
    "css-utilties",
    "css-helpers"
  ],
  "license": "MIT",
  "dependencies": {
    "postcss-calc": "^7.0.5",
    "postcss-conditionals": "^2.1.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^10.0.0",
    "postcss-each": "^0.10.0",
    "postcss-import-sync2": "^1.1.0",
    "postcss-media-variables": "^2.0.1",
    "postcss-merge-rules": "^4.0.3",
    "postcss-nested": "^4.2.3",
    "postcss-percentage": "0.0.0",
    "postcss-preset-env": "^6.7.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "parcel-plugin-stylelint-all": "^0.2.0"
  },
  "gitHead": "1660a38819c8dbad053d9164caa58300bf1d60b1"
}
