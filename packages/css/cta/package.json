{
  "name": "@novicell/styles-cta",
  "version": "0.1.2",
  "description": "The stylesheet for Novicell CTA as compiled CSS and raw postCSS.",
  "author": "Magnus Bendix Borregaard <mbb@novicell.dk>",
  "homepage": "https://github.com/Novicell/frontend-packages#readme",
  "license": "MIT",
  "main": "dist/index.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Novicell/frontend-packages.git"
  },
  "private": true,
