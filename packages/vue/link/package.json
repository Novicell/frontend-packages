{
  "name": "@novicell/link",
  "version": "0.1.0",
  "description": "A Vue link component, that automatically switches between and anchor-tag and NuxtLink, depending on whether the provided link is internal or external.",
  "repository": {
    "url": "https://github.com/Novicell/frontend-packages"
  },
  "main": "dist/link.umd.js",
  "module": "dist/link.esm.js",
  "unpkg": "dist/link.min.js",
  "browser": {
    "./sfc": "src/link.vue"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "help": "vue serve --help",
    "dev": "vue serve --open src/link.vue",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config config/rollup.config.js --format umd --file dist/link.umd.js",
    "build:es": "rollup --config config/rollup.config.js --format es --file dist/link.esm.js",
    "build:unpkg": "rollup --config config/rollup.config.js --format iife --file dist/link.min.js",
    "prepublishOnly": "npm run build"
  },
  "author": "Magnus Bendix Borregaard <mbb@novicell.dk>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@vue/cli": "^4.5.8",
    "@vue/cli-service-global": "^4.5.8",
    "rollup": "~2.33.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "2.6.12",
    "vue-template-compiler": "^2.6.12"
  }
}
